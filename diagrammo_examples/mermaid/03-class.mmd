%% +public, -private, #protected
%% relationships: --> association, --* composition, --o aggregation
classDiagram
    class Captain {
        +String name
        +int reputation
        +commandShip()
        +divideLoot()
        +walkThePlank(crew)
    }
    class Quartermaster {
        +int crewCount
        +resolveDisputes()
        +distributeLoot()
        +maintainDiscipline()
    }
    class Navigator {
        +String[] charts
        +plotCourse()
        +readStars()
        +avoidNavy()
    }
    class Gunner {
        +int cannonCount
        +fireBroadside()
        +maintainWeapons()
    }
    class Surgeon {
        +String[] medicines
        +treatWounds()
        +amputateLimbs()
    }
    class Boatswain {
        +inspectRigging()
        +superviseDeck()
        +punishSlackers()
    }

    Captain "1" --> "1" Quartermaster : commands
    Captain "1" --> "1" Navigator : relies on
    Quartermaster "1" --> "*" Gunner : oversees
    Quartermaster "1" --> "1" Surgeon : assigns
    Quartermaster "1" --> "1" Boatswain : directs